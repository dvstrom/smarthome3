<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="6.0dip" />
            <gradient android:startColor="#ffe9eeef" android:endColor="#ff808a87" android:angle="270.0" android:centerY="0.45" android:centerColor="#ffe9eeef" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="6.0dip" />
                <gradient android:startColor="#ff12a0fa" android:endColor="#ff12a0fa" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff12a0fa" />
            </shape>
        </clip>
    </item>
</layer-list>
